{
  "name": "dashcam-api",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Aleksei Punov (Hivemapper)",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/connect-busboy": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/node": "^18.6.1",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-jest": "^26.6.3",
    "copyfiles": "^2.4.1",
    "eslint": "7.28.0",
    "lint-staged": "^12.3.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "busboy": "^1.6.0",
    "child_process": "^1.0.2",
    "connect-busboy": "^1.0.0",
    "console-stamp": "^3.0.6",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "shortid": "^2.2.16",
    "tslint": "^6.1.3"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "compile": "rm -rf dist && sed -i '' \"s/hdc'/$npm_config_camera'/g\" src/config/index.ts && tsc && rm -rf compiled/odc-api-$npm_config_camera.js && ncc build dist/index.js -o compiled && mv compiled/index.js compiled/odc-api-$npm_config_camera.js && copyfiles -u 2 src/fixtures/* compiled/fixtures && sed -i '' \"s/$npm_config_camera'/hdc'/g\" src/config/index.ts",
    "start-compiled": "node compiled/dashcam-api.js",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "lint": "eslint --fix",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "pre-commit": "lint-staged"
  }
}
